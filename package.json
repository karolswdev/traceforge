{
  "name": "traceforge",
  "version": "0.1.0",
  "description": "Traceforge: scaffold nested, multi-agent orchestration for Claude Code (+Codex/Gemini) and generate SRS/phase plans",
  "license": "MIT",
  "bin": { "traceforge": "dist/index.js" },
  "type": "module",
  "main": "dist/index.js",
  "files": ["dist", "README.md", "LICENSE", "CONTRIBUTING.md", "CODE_OF_CONDUCT.md"],
